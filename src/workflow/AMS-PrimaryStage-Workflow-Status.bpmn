if(primaryStatus.equals("Review")){
	statusList.add("OnHold");
	statusList.add("Rework");
	statusList.add("PQA_Review");
	statusList.add("Completed");
}
kcontext.setVariable("statusList",statusList);
</script>
    </scriptTask>
